<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    
    <!-- Google Play Services permissions start -->
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <!-- Google Play Services permissions end -->

    <permission
            android:name="org.nativescript.sampleGooglePlayServices.permission.MAPS_RECEIVE"
            android:protectionLevel="signature"/>
    <uses-permission android:name="org.nativescript.sampleGooglePlayServices.permission.MAPS_RECEIVE"/>
    
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    
    <application>
        <meta-data android:name="com.google.android.gms.version"
    android:value="@integer/google_play_services_version" />
                
        <!-- API_KEY is per app, per developer, generate one following these steps - https://developers.google.com/maps/documentation/android/signup -->
        <meta-data android:name="com.google.android.geo.API_KEY" android:value="YOUR_API_KEY_HERE"/>
    </application>
</manifest>
